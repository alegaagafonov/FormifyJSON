{
  "name": "@formify-json/form-generator",
  "version": "0.0.5",
  "description": "JSON form builder for React",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alegaagafonov/FormifyJSON.git"
  },
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "watch": "npm run watch:esm & npm run watch:cjs",
    "watch:esm": "tsc --watch",
    "watch:cjs": "tsc --module commonjs --outDir dist/cjs --watch",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "react",
    "typescript",
    "form-builder",
    "json-form"
  ],
  "author": "alexander",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^28.1.1",
    "@types/react": "^18.0.12",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@formify-json/types-and-schemas": "^0.0.5",
    "@hookform/resolvers": "^3.1.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.1",
    "react-hook-form": "^7.43.9",
    "zod": "^3.21.4"
  },
  "gitHead": "7e6e6740f77aefbd9a15d65dd4abf51c00e33eb6"
}
